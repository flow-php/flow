{
    "name": "flow-php/etl-adapter-reactphp",
    "type": "library",
    "description": "PHP ETL - Asynchronous Adapter on ReactPHP",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "async",
        "reactphp"
    ],
    "require": {
        "php": "~8.1 || ~8.2",
        "flow-php/etl": "^0.2 || 1.x-dev",
        "monolog/monolog": "^3.0",
        "react/child-process": "^0.6.4",
        "react/socket": "^1.11"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": [
        "bin/worker-reactphp"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

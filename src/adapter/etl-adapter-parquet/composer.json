{
    "name": "flow-php/etl-adapter-parquet",
    "type": "library",
    "description": "PHP ETL - Adapter - Parquet",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "parquet",
        "adapter"
    ],
    "require": {
        "php": "~8.1 || ~8.2",
        "ext-json": "*",
        "codename/parquet": "~0.6.2 || ~0.7.0",
        "flow-php/etl": "^0.4 || 1.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

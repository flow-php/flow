{
    "name": "flow-php/etl-adapter-amphp",
    "type": "library",
    "description": "PHP ETL - Asynchronous Adapter on amphp",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "async",
        "amphp"
    ],
    "require": {
        "php": "~8.1 || ~8.2",
        "amphp/process": "^2",
        "amphp/socket": "^2",
        "flow-php/etl": "^0.2 || 1.x-dev",
        "monolog/monolog": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": [
        "bin/worker-amp"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

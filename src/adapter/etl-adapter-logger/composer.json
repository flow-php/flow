{
    "name": "flow-php/etl-adapter-logger",
    "type": "library",
    "description": "PHP ETL - Adapter - Logger",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "logger",
        "psr"
    ],
    "require": {
        "php": "~8.1 || ~8.2 || ~8.3",
        "flow-php/etl": "^0.4 || 1.x-dev",
        "psr/log": "^2.0 || ^3.0"
    },
    "require-dev": {
        "fig/log-test": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

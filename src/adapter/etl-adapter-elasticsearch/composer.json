{
    "name": "flow-php/etl-adapter-elasticsearch",
    "type": "library",
    "description": "PHP ETL - Adapter - Elasticsearch",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "elasticsearch",
        "adapter"
    ],
    "require": {
        "php": "~8.1 || ~8.2",
        "ext-hash": "*",
        "ext-json": "*",
        "elasticsearch/elasticsearch": "^7.6|^8.0",
        "flow-php/etl": "^0.2 || 1.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

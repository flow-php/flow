{
    "name": "flow-php/etl-adapter-avro",
    "type": "library",
    "description": "PHP ETL - Adapter - Avro",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "avro",
        "adapter"
    ],
    "require": {
        "php": "~8.1 || ~8.2 || ~8.3",
        "ext-json": "*",
        "flix-tech/avro-php": "~4.2.0 || ~4.3.0",
        "flow-php/etl": "^0.4 || 1.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        },
        "files": [
            "src/Flow/ETL/Adapter/Avro/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

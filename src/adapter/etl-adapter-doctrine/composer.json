{
    "name": "flow-php/etl-adapter-doctrine",
    "type": "library",
    "license": "MIT",
    "description": "PHP ETL - Adapter - Doctrine Dbal",
    "keywords": [
        "doctrine",
        "dbal",
        "bulk",
        "insert",
        "upsert",
        "etl",
        "loader"
    ],
    "require": {
        "php": "~8.1 || ~8.2",
        "flow-php/doctrine-dbal-bulk": "^0.2 || 1.x-dev",
        "flow-php/etl": "^0.2 || 1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "flow-php/etl-adapter-doctrine",
    "type": "library",
    "license": "MIT",
    "description": "PHP ETL - Adapter - Doctrine Dbal",
    "keywords": [
        "doctrine",
        "dbal",
        "bulk",
        "insert",
        "upsert",
        "etl",
        "loader"
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "doctrine/dbal": "^3.6 || ^4.0",
        "flow-php/doctrine-dbal-bulk": "^0.7 || 1.x-dev",
        "flow-php/etl": "^0.7 || 1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        },
        "files": [
            "src/Flow/ETL/Adapter/Doctrine/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

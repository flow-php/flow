{
    "name": "flow-php/etl-adapter-meilisearch",
    "type": "library",
    "description": "PHP ETL - Adapter - Meilisearch",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "elasticsearch",
        "adapter"
    ],
    "require": {
        "php": "~8.1 || ~8.2 || ~8.3",
        "ext-hash": "*",
        "ext-json": "*",
        "meilisearch/meilisearch-php": "^1.1",
        "flow-php/etl": "^0.5.0 || 1.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        },
        "files": [
            "src/Flow/ETL/Adapter/Meilisearch/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

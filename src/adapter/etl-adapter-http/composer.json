{
    "name": "flow-php/etl-adapter-http",
    "type": "library",
    "description": "PHP ETL - Adapter - Http",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "http"
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "ext-json": "*",
        "flow-php/etl": "^0.5.0 || 1.x-dev",
        "psr/http-client": "^1.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "php-http/curl-client": "^2.2"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "flow-php/parquet-viewer",
    "type": "library",
    "description": "PHP ETL - cli application for viewing parquet files",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "filter",
        "parquet"
    ],
    "require": {
        "php": "~8.1 || ~8.2",
        "coduo/php-humanizer": "^4.0",
        "flow-php/etl": "^0.4 || 1.x-dev",
        "flow-php/parquet": "^0.4 || 1.x-dev",
        "symfony/console": "^6.3"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "flow-php/array-dot",
    "type": "library",
    "description": "PHP ETL - Array Dot functions",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "filter",
        "array",
        "dot"
    ],
    "require": {
        "php": "~8.1 || ~8.2 || ~8.3"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        },
        "files": [
            "src/Flow/ArrayDot/array_dot.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

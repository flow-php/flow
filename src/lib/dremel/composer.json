{
    "name": "flow-php/dremel",
    "type": "library",
    "description": "PHP ETL - Dremel algorithm implementation",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "filter",
        "dremel",
        "algorithm"
    ],
    "require": {
        "php": "~8.1 || ~8.2"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

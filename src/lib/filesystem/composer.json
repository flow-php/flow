{
    "name": "flow-php/filesystem",
    "type": "library",
    "description": "PHP ETL - Filesystem abstraction",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "filesystem",
        "remote",
        "azure",
        "aws",
        "gcp"
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "webmozart/glob": "^3.0 || ^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        },
        "files": [
            "src/Flow/Filesystem/DSL/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

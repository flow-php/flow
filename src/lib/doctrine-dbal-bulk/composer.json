{
    "name": "flow-php/doctrine-dbal-bulk",
    "type": "library",
    "license": "MIT",
    "description": "Bulk inserts and updates for Doctrine DBAL",
    "keywords": [
        "doctrine",
        "dbal",
        "bulk",
        "insert",
        "upsert"
    ],
    "require": {
        "php": "~8.1 || ~8.2 || ~8.3",
        "doctrine/dbal": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Flow\\Doctrine\\Bulk\\": [
                "src/Flow/Doctrine/Bulk"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\Doctrine\\Bulk\\Tests\\": [
                "tests/Flow/Doctrine/Bulk/Tests"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}

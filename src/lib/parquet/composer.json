{
    "name": "flow-php/parquet",
    "type": "library",
    "description": "PHP ETL - library for reading and writing Parquet files",
    "keywords": [
        "etl",
        "extract",
        "transform",
        "load",
        "filter",
        "parquet"
    ],
    "require": {
        "php": "~8.1 || ~8.2 || ~8.3",
        "ext-bcmath": "*",
        "ext-zlib": "*",
        "composer-runtime-api": "^2.1",
        "flow-php/dremel": "^0.5.0 || 1.x-dev",
        "flow-php/snappy": "^0.5.0 || 1.x-dev",
        "packaged/thrift": "^0.15.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        },
        "files": [
            "src/Flow/Parquet/functions.php",
            "src/Flow/Parquet/polyfill.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

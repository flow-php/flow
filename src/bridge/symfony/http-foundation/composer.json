{
    "name": "flow-php/symfony-http-foundation-bridge",
    "type": "library",
    "description": "PHP ETL - Symfony HTTP foundation bridge",
    "keywords": [
        "symfony",
        "http",
        "foundation",
        "bridge",
        "streamed",
        "response"
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "symfony/http-foundation": "~5.4.0 || ~6.4.0 || ~7",
        "flow-php/etl": "^0.9.0 || 1.x-dev"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flow\\": [
                "src/Flow"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flow\\": "tests/Flow"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

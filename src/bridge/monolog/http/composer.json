{
  "name": "flow-php/monolog-http-bridge",
  "type": "library",
  "description": "PHP ETL - Monolog HTTP bridge",
  "keywords": [
    "logs",
    "monolog",
    "http",
    "bridge"
  ],
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "monolog/monolog": "^2.0||^3.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "nyholm/psr7": "^1.8"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Flow\\": [
        "src/Flow"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flow\\": "tests/Flow"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

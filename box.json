{
  "alias": "flow.phar",
  "banner": false,
  "blacklist": [
    ".github",
    "examples",
    "docs",
    "tests",
    "tools",
    "var",
    "fakerphp",
    "fig",
    "mock-client",
    "dotenv",
    "var-exporter"
  ],
  "compression": "GZ",
  "directories": [
    "build",
    "src",
    "vendor"
  ],
  "exclude-dev-files": false,
  "git-version": "git_version",
  "main": "bin/flow.php",
  "metadata": "FlowVersion::getVersion",
  "output": "build/flow.phar"
}

{
  "timeout": 1000,
  "source": {
    "directories": [
      "src\/Flow"
    ],
    "excludes": [
      "/.*\\.phpt/"
    ]
  },
  "logs": {
    "text": "./var/infection/infection.log",
    "summary": "./var/infection/infection_summary.log",
    "debug": "./var/infection/infection_summary.log",
    "badge": {
      "branch": "1.x"
    }
  },
  "mutators": {
    "@default": true
  },
  "testFramework": "phpunit",
  "bootstrap": "./vendor/autoload.php",
  "minMsi": 60,
  "minCoveredMsi": 60
}